{
    "name": "theme/scottshealy",
    "description": "Setup required assets for theme",
    "type": "wordpress-theme",
    "minimum-stability": "stable",
    
    "authors": [
        {
            "name": "Scott Shealy",
            "email": "vsshealy@gmail.com"
        }
