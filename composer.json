        "wpackagist-plugin/breadcrumb-navxt": "*",
        "wpackagist-plugin/buddypress": "*",
        "wpackagist-plugin/classic-editor": "*",
        "wpackagist-plugin/code-snippets": "*",
        "wpackagist-plugin/contact-form-7": "*",
        "wpackagist-plugin/contact-form-7-multi-step-module": "*",
        "wpackagist-plugin/cf7-conditional-fields": "*",
        "wpackagist-plugin/custom-post-type-permalinks": "*",
